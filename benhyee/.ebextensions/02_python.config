container_commands:
  01_migrate:
    command: "source /opt/python/run/venv/bin/activate && python benhyee/manage.py migrate --noinput"
    leader_only: true




option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "benhyee.settings"
    "PYTHONPATH": "/opt/python/current/app/benhyee:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: benhyee/benhyee/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"